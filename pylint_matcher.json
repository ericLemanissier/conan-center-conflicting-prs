{
    "problemMatcher": [
        {
            "owner": "pylint_fatals",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(F.+):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "pylint_errors",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(E.+):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "pylint_warnings",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(W.+):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
